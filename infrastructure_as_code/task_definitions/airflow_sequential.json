[
  {
    "name": "webserver",
    "links": [
      "postgres"
    ],
    "image": "puckel/docker-airflow",
    "essential": true,
    "environment": [
      {
        "name": "LOAD_EX",
        "value": "n"
      }
    ],
    "portMappings": [
      {
        "containerPort": 8080,
        "hostPort": 8080
      }
    ],
    "memory": 400,
    "cpu": 7
  },
  {
    "name": "postgres",
    "image": "postgres",
    "environment": [
      {
        "name": "POSTGRES_USER",
        "value": "airflow"
      },
      {
        "name": "POSTGRES_PASSWORD",
        "value": "airflow"
      },
      {
        "name": "POSTGRES_DB",
        "value": "airflow"
      }
    ],
    "memory": 400,
    "cpu": 7
  }
]